{
  "name": "p5.party",
  "version": "0.7.1",
  "author": "Justin Bakse",
  "license": "MIT",
  "description": "Pre-release! An easy to use library for simple multi-user sketches with p5.js.",
  "keywords": [
    "p5",
    "multiuser",
    "multiplayer",
    "library"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "dist"
  ],
  "main": "src/index_p5.js",
  "sideEffects": [
    "./src/index.p5.js",
    "./src/InfoPanel.css"
  ],
  "engines": {
    "node": "16.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jbakse/deepstream_test.git"
  },
  "scripts": {
    "prestart": "npm run gv",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "prebuild": "npm run gv",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "profile": "webpack --config webpack.profile.js",
    "serve": "PORT=${PORT:-6020} deepstream start",
    "test": "jest --coverage",
    "!posttest": "istanbul-badges-readme",
    "gv": "genversion --es6 --semi version.js",
    "prezip": "rm -f p5.party.zip",
    "zip": "npm run build && ln -s public/examples && zip p5.party.zip -r dist -r examples && rm -r examples && unzip -l p5.party.zip",
    "prerelease": "npm run build",
    "release": "np",
    "prepublishOnly": "npm run build && npm run zip",
    "postpublish": "curl https://purge.jsdelivr.net/npm/p5.party@latest/dist/p5.party.js",
    "netlify": "npm run build && cp -r dist public"
  },
  "dependencies": {
    "@deepstream/client": "^5.2.6",
    "@deepstream/server": "^5.2.6",
    "core-js": "^3.20.2",
    "on-change": "^4.0.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "babel-jest": "^27.5.0",
    "babel-loader": "^8.2.3",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.6.0",
    "eslint-config-p5js": "^1.1.0",
    "genversion": "^3.0.2",
    "istanbul-badges-readme": "^1.8.1",
    "jest": "^27.5.0",
    "jest-css-modules": "^2.1.0",
    "np": "^7.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.66.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  }
}
